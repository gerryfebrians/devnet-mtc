using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace assesment
{
    class Program
    {
        static void Main(string[] args)
        {

            const string nama = "compnet";
            string cetak = "";

            for (int i = 1; i < 10; i++)
            {
                Console.Write(i + " ");
                for (int j = 1; j <= i; j++)
                {
                    Console.Write(j);
                }

                Console.WriteLine();
            }
                        
            Console.WriteLine(new string('-',30));
            cetak = nama == "compnet" ? "Nama perusahaan kita" : "Bukan nama perusaah kita";

            Console.WriteLine($"{nama} adalah {cetak}");

           
        }

    }
}
